#include <iostream>
#include <map>
#include <vector>
#include <algorithm>
using namespace std;
int main(){
	int n;
	cin >> n;
	string s;
	for(int i = 0; i < n; i++){
		char c;
		cin >> c;
		s += c;
	}
	char b;
	cin >> b;
	for(int i = 0;i < n; i++){
		if(b < s[i]){
			cout << s[i];
			return 0;
		}
	}
	cout << s[0];
}
